package it.prova.gestionemunicipiospringjpa.service.municipio;

import java.util.List;

import it.prova.gestionemunicipiospringjpa.model.Contribuente;

public interface MunicipioService {
	
	public List<Contribuente> listAllMunicipi() ;

	public Contribuente caricaSingoloMunicipio(Long id);

	public void aggiorna(Contribuente municipioInstance);

	public void inserisciNuovo(Contribuente municipioInstance);

	public void rimuovi(Contribuente municipioInstance);

	public List<Contribuente> findByExample(Contribuente example);
	
	public List<Contribuente> cercaByDescrizioneILike(String term);

}
